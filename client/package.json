  "devDependencies": {
    "babel-loader": "^8.0.6",
    "circular-dependency-plugin": "^5.0.2",
    "isomorphic-style-loader": "^5.1.0",
    "prop-types": "^15.6.2",
    "react-router-dom": "^4.3.1",
    "serverless-api-compression": "^1.0.1",
    "serverless-domain-manager": "^3.2.6",
    "svg-inline-loader": "^0.8.0",
    "webpack-cli": "^3.3.6"
  }
