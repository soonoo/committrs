{
  "name": "committrs-client",
  "version": "0.1.0",
  "homepage": "https://committrs.io",
  "private": true,
  "dependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "history": "^4.9.0",
    "react": "^16.8.3",
    "react-content-loader": "^4.2.1",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.3",
    "react-testing-library": "^5.4.4",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-saga": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn build && aws s3 cp build s3://$COMMITTRS_S3_URL/ --recursive",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $COMMITTRS_CLOUDFRONT_DIST_ID --paths '/*'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "prop-types": "^15.6.2",
    "react-router-dom": "^4.3.1"
  }
}
